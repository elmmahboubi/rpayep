{
  "version": 2,
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/client/assets/$1"
    },
    {
      "src": "/(.*\\.(?:ico|svg|png|webp|woff2))$",
      "dest": "/dist/client/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server/index.ts"
    }
  ]
} 