{
  "version": 2,
  "framework": null,
  "functions": {
    "index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/client/assets/$1"
    },
    {
      "src": "/(.*\\.(ico|svg|png|webp|woff2))",
      "dest": "/dist/client/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "outputDirectory": "."
} 